[
    {
        "id" : "step-1",
        "fields" : [
            {
                "title" : "Seit wann verkaufst du aktiv auf Amazon?",
                "type" : "date",
                "placeholder" : "01.06.2019",
                "validate" : {
                    "notEmpty" : true,
                    "lessDate" : [
                        "%%NOW - 12MON",
                        "Ihr Unternehmen muss mind. 12 Monate alt für einen möglichen Verkauf"
                    ],
                    "regExp" : [
                        "^\\d{2}\\.\\d{2}\\.\\d{4}$"
                    ]
                }
            },
            {
                "title" : "Anteil Amazon Umsatz in Prozent von Gesamtumsatz",
                "type" : "precent",
                "placeholder" : "60",
                "description" : "Umsatz deiner über Amazon erzielten Verkäufe im Verhältnis zum Gesamtumsatz.",
                "validate" : {
                    "notEmpty" : true,
                    "integer" : true,
                    "min" : [
                        60,
                        "Dieser Unternehmensrechner ist für Amazon Unternehmen entwickelt worden, die mind. 60% des Gesmatumsatzes über Amazon generieren."
                    ],
                    "max" : [100]
                }
            }
        ]
    },
    {
        "id" : "step-2",
        "fields" : [
            {
                "title" : "Wie hoch ist dein durchschnittlicher monatlicher Nettoumsatz?",
                "type" : "euro",
                "placeholder" : "0",
                "validate" : {
                    "notEmpty" : true,
                    "euro" : true
                }
            },
            {
                "title" : "Wie hoch ist dein durchschnittliches monatliches Betriebsergebnis (EBITDA)?",
                "type" : "euro",
                "description" : "Gewinn nach Abzug aller variablen und fixen Kosten. Ohne Abzug von Steuern, Zinsen und Unternehmerlohn.",
                "placeholder" : "0",
                "validate" : {
                    "notEmpty" : true,
                    "euro" : true
                }
            }
        ]
    },
    {
        "id" : "step-3",
        "fields" : [
            {
                "title" : "Anzahl der bei Amazon gelisteten Produkte (ohne Varianten)",
                "type" : "select",
                "options" : {
                    "5" : "Weniger als 10",
                    "10" : "10 bis 25",
                    "25" : "25 bis 50",
                    "50" : "50 bis 100",
                    "100" : "100 bis 250",
                    "250" : "250 bis 500",
                    "500" : "500 bis 1.000",
                    "1000" : "1.000 bis 2.500",
                    "2500" : "2.500 bis 5.000",
                    "5000" : "5.000 bis 10.000",
                    "10000" : "Mehr als 10.000"
                },
                "validate" : {
                    "notEmpty" : true,
                    "integer" : true
                }
            },
            {
                "title" : "Anteil von Eigenmarken an den gelisteten Produkten in Prozent",
                "type" : "precent",
                "validate" : {
                    "notEmpty" : true,
                    "integer" : true,
                    "max" : [100]
                }
            }
        ]
    }
]
